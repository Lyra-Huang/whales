@ARTICLE{Ottewell2016193,
author={Ottewell, K. and Coughran, D. and Gall, M. and Irvine, L. and Byrne, M.},
title={A recent stranding of Omura's whale (Balaenoptera omurai) in Western Australia},
journal={Aquatic Mammals},
