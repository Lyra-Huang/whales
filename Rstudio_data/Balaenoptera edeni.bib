@ARTICLE{Iwanami2006249,
author={Iwanami, K. and Mita, H. and Yamamoto, Y. and Fujise, Y. and Yamada, T. and Suzuki, T.},
title={cDNA-derived amino acid sequences of myoglobins from nine species of whales and dolphins},
journal={Comparative Biochemistry and Physiology - B Biochemistry and Molecular Biology},
year={2006},
volume={145},
number={2},
pages={249-256},
doi={10.1016/j.cbpb.2006.08.002},
note={cited By 12},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-33748908118&doi=10.1016%2fj.cbpb.2006.08.002&partnerID=40&md5=d6dbc9a1113075b1a0691b857a6a14a6},
affiliation={Laboratory of Biochemistry, Faculty of Science, Kochi University, Kochi, 780-8520, Japan; Department of Chemistry, University of Tsukuba, Tsukuba, 305-8571, Japan; The Institute of Cetacean Research, Tokyo, 104-0055, Japan; National Science Museum, Tokyo, 169-0073, Japan},
abstract={We determined the myoglobin (Mb) cDNA sequences of nine cetaceans, of which six are the first reports of Mb sequences: sei whale (Balaenoptera borealis), Bryde's whale (Balaenoptera edeni), pygmy sperm whale (Kogia breviceps), Stejneger's beaked whale (Mesoplodon stejnegeri), Longman's beaked whale (Indopacetus pacificus), and melon-headed whale (Peponocephala electra), and three confirm the previously determined chemical amino acid sequences: sperm whale (Physeter macrocephalus), common minke whale (Balaenoptera acutorostrata) and pantropical spotted dolphin (Stenella attenuata). We found two types of Mb in the skeletal muscle of pantropical spotted dolphin: Mb I with the same amino acid sequence as that deposited in the protein database, and Mb II, which differs at two amino acid residues compared with Mb I. Using an alignment of the amino acid or cDNA sequences of cetacean Mb, we constructed a phylogenetic tree by the NJ method. Clustering of cetacean Mb amino acid and cDNA sequences essentially follows the classical taxonomy of cetaceans, suggesting that Mb sequence data is valid for classification of cetaceans at least to the family level. © 2006 Elsevier Inc. All rights reserved.},
